
std :: import std;
windows :: import windows;

mainCRTStartup :: func ()
{
    exit_code := main();
    @exit(exit_code);
}

main :: func () -> s64
{
    stdout_handle := windows.GetStdHandle(windows.WINDOWS_STDOUT_HANDLE_NUM);

    written : windows.DWORD;
    str := "Hello World!\n";
    len := @cast(windows.DWORD, std.strlen(str));
    windows.WriteFile(stdout_handle, str, len, *written, null);

    return 41;
}
