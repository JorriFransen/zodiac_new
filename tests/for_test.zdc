test_array_for :: func()
{
    int_array : [5]s64;

    for (i := 0; i < int_array.count; i = i + 1)
    {
        int_array[i] = i;
    }

    for (j := 0; j < int_array.count; j = j + 1)
    {
        std.print("int_array[");
        std.print_num(j);
        std.print("] = ");
        std.print_num(int_array[j]);
        std.print("\n");
    }

    return;
}

