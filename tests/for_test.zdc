
std :: import std;

main :: func() -> s64
{
    std.println("for test");

    for (i : s64 = 0; i < 10; i = i + 1)
    {
        std.print_num(i);
        std.print("\n");
    }

    std.print("\n");

    for (i := 9; i >= 0; i = i - 1)
    {
        std.print_num(i);
        std.print("\n");
    }

    std.print("\n");

    return 0;
}

#naked #noreturn _start :: func ()
{
    posix_call_main_and_exit();
}

#noreturn posix_call_main_and_exit :: func ()
{
    exit_code := main();
    @exit(exit_code);
}
