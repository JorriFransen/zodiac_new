
Vec2 :: struct 
{
    x : s64;
    y : s64;
}

main :: func () -> s64
{
    p1 : Vec2;
    p1.x = 33;
    p1.y = 44;

    p2 : Vec2;
    p2.x = p1.y;
    p2.y = p1.x;

    p3 : Vec2;
    p3 = p2;

    return p3.x + p3.y;
}

#foreign #noreturn ExitProcess :: func (exit_code: u32) -> void

mainCRTStartup :: func () -> s32
{
    exit_code := main();
    @exit(exit_code);
}

#naked #noreturn _start :: func ()
{
    posix_call_main_and_exit();
}

#noreturn posix_call_main_and_exit :: func ()
{
    exit_code := main();
    @exit(exit_code);
}
